CREATE DATABASE CEPREUNAJ;

USE CEPREUNAJ;

CREATE TABLE Estudiante (
  DNI INT NOT NULL,
  Primer_nombre VARCHAR(50) NOT NULL,
  Apellido_paterno VARCHAR(50) NOT NULL,
  Apellido_materno VARCHAR(50) NOT NULL,
  PRIMARY KEY (DNI)
);

CREATE TABLE Segundo_nombre_del_estudiante (
  Segundo_nombre VARCHAR(50),
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI)
);

CREATE TABLE Tercer_nombre_del_estudiante (
  Tercer_nombre VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI)
);

CREATE TABLE Numero_de_celular_del_estudiante (
  Numero_de_celular VARCHAR(15) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI)
);

CREATE TABLE Correo_electronico_del_estudiante (
  Correo_electronico VARCHAR(100) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI)
);

CREATE TABLE Docente (
  DNI INT NOT NULL,
  Primer_nombre VARCHAR(50) NOT NULL,
  Apellido_paterno VARCHAR(50) NOT NULL,
  Apellido_materno VARCHAR(50) NOT NULL,
  PRIMARY KEY (DNI)
);

CREATE TABLE Segundo_nombre_del_docente (
  Segundo_nombre VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Docente(DNI)
);

CREATE TABLE Tercer_nombre_del_docente (
  Tercer_nombre VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Docente(DNI)
);

CREATE TABLE Correo_electronico_del_docente (
  Correo_electronico VARCHAR(100) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Docente(DNI)
);

CREATE TABLE Numero_de_celular_del_docente (
  Numero_de_celular VARCHAR(15) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Docente(DNI)
);

CREATE TABLE Personal (
  DNI INT NOT NULL,
  Primer_nombre VARCHAR(50) NOT NULL,
  Apellido_paterno VARCHAR(50) NOT NULL,
  Apellido_materno VARCHAR(50) NOT NULL,
  PRIMARY KEY (DNI)
);

CREATE TABLE Segundo_nombre_del_personal (
  Segundo_nombre VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Personal(DNI)
);

CREATE TABLE Tercer_nombre_del_personal (
  Tercer_nombre VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  UNIQUE KEY (DNI),
  FOREIGN KEY (DNI) REFERENCES Personal(DNI)
);

CREATE TABLE Correo_electronico_del_personal (
  Correo_electronico VARCHAR(100) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Personal(DNI)
);

CREATE TABLE Numero_de_celular_del_personal (
  Numero_de_celular VARCHAR(15) NOT NULL,
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Personal(DNI)
);

CREATE TABLE Periodo_academico (
  Id_del_periodo INT NOT NULL,
  Nombre VARCHAR(100) NOT NULL,
  PRIMARY KEY (Id_del_periodo)
);

CREATE TABLE Asignatura (
  Id_de_la_asignatura INT NOT NULL,
  Nombre VARCHAR(100) NOT NULL,
  Id_del_periodo INT NOT NULL,
  PRIMARY KEY (Id_de_la_asignatura),
  FOREIGN KEY (Id_del_periodo) REFERENCES Periodo_academico(Id_del_periodo)
);

CREATE TABLE Horario_de_la_Asignatura (
  Dia VARCHAR (10) NOT NULL,
  Hora_de_inicio TIME NOT NULL,
  Hora_de_termino TIME NOT NULL,
  Id_de_la_asignatura INT NOT NULL,
  FOREIGN KEY (Id_de_la_asignatura) REFERENCES Asignatura(Id_de_la_asignatura)
);

CREATE TABLE Docente_encargado_de_la_Asignatura (
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Docente(DNI),
  Id_de_la_asignatura INT NOT NULL,
  FOREIGN KEY (Id_de_la_asignatura) REFERENCES Asignatura(Id_de_la_asignatura)
);

CREATE TABLE Aula (
  Codigo_del_aula INT NOT NULL,
  Nombre VARCHAR(20) NOT NULL,
  PRIMARY KEY (Codigo_del_aula)
);

CREATE TABLE Grupo (
  Id_del_grupo INT NOT NULL,
  Turno VARCHAR(15) NOT NULL,
  Numero_de_estudiantes INT NOT NULL,
  Nombre VARCHAR(30) NOT NULL,
  PRIMARY KEY (Id_del_grupo),
  Codigo_del_aula INT NOT NULL,
  FOREIGN KEY (Codigo_del_aula) REFERENCES Aula(Codigo_del_aula)
);

CREATE TABLE Grupo_al_que_pertenece_el_estudiante (
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI),
  Id_del_grupo INT NOT NULL,
  FOREIGN KEY (Id_del_grupo) REFERENCES Grupo(Id_del_grupo)
);

CREATE TABLE Asignaturas_imparte_el_docente_en_el_grupo (
  DNI INT NOT NULL,
  FOREIGN KEY (DNI) REFERENCES Docente(DNI),
  Id_del_grupo INT NOT NULL,
  FOREIGN KEY (Id_del_grupo) REFERENCES Grupo(Id_del_grupo),
  Id_de_la_asignatura INT NOT NULL,
  FOREIGN KEY (Id_de_la_asignatura) REFERENCES Asignatura(Id_de_la_asignatura)
);

CREATE TABLE Control_de_ingreso_al_examen (
  Id_Del_control INT NOT NULL,
  Numero_De_puerta INT NOT NULL,
  Fecha DATE NOT NULL,
  Hora TIME NOT NULL,
  DNI_Personal INT NOT NULL,
  DNI_Estudiante INT NOT NULL,
  PRIMARY KEY (Id_Del_control),
  FOREIGN KEY (DNI_Personal) REFERENCES Personal(DNI),
  FOREIGN KEY (DNI_Estudiante) REFERENCES Estudiante(DNI)
);

CREATE TABLE Clases (
  Id_de_las_clases INT NOT NULL,
  Fecha DATE NOT NULL,
  Id_de_la_asignatura INT NOT NULL,
  Id_del_grupo INT NOT NULL,
  PRIMARY KEY (Id_de_las_clases),
  FOREIGN KEY (Id_de_la_asignatura) REFERENCES Asignatura(Id_de_la_asignatura),
  FOREIGN KEY (Id_del_grupo) REFERENCES Grupo(Id_del_grupo)
);

CREATE TABLE Matricula (
  Id_de_matricula INT NOT NULL,
  Fecha DATE NOT NULL,
  Carrera VARCHAR(100) NOT NULL,
  Monto DECIMAL(10, 2) NOT NULL,
  Metodo_de_pago VARCHAR(50) NOT NULL,
  DNI INT NOT NULL,
  Id_del_periodo INT NOT NULL,
  PRIMARY KEY (Id_de_matricula),
  FOREIGN KEY (DNI) REFERENCES Estudiante(DNI),
  FOREIGN KEY (Id_del_periodo) REFERENCES Periodo_academico(Id_del_periodo)
);

CREATE TABLE Examen_CEPRE (
  Id_de_examen INT NOT NULL,
  Fecha DATE NOT NULL,
  Hora_de_inicio TIME NOT NULL,
  Hora_de_termino TIME NOT NULL,
  Numero_de_preguntas INT NOT NULL,
  Id_del_periodo INT NOT NULL,
  PRIMARY KEY (Id_de_examen),
  FOREIGN KEY (Id_del_periodo) REFERENCES Periodo_academico(Id_del_periodo)
);

