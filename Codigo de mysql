-- Procedimiento para insertar en la tabla Estudiante
DELIMITER //
CREATE PROCEDURE InsertarEstudiante(
    IN p_DNI INT,
    IN p_Primer_nombre VARCHAR(50),
	IN p_Segundo_nombre VARCHAR(50),
    IN p_Apellido_paterno VARCHAR(50),
    IN p_Apellido_materno VARCHAR(50),
    IN p_Numero_de_celular VARCHAR(9),
    IN p_Correo_electronico VARCHAR(100)
)
BEGIN
    INSERT INTO Estudiante (DNI, Primer_nombre, Apellido_paterno, Apellido_materno) 
    VALUES (p_DNI, p_Primer_nombre, p_Apellido_paterno, p_Apellido_materno);
    INSERT INTO Segundo_nombre_del_estudiante (Segundo_nombre, DNI) 
    VALUES (p_Segundo_nombre, p_DNI);
    INSERT INTO Numero_de_celular_del_estudiante (Numero_de_celular, DNI) 
    VALUES (p_Numero_de_celular, p_DNI);
    INSERT INTO Correo_electronico_del_estudiante (Correo_electronico, DNI) 
    VALUES (p_Correo_electronico, p_DNI);
END //
DELIMITER ;

CALL InsertarEstudiante (7557487,'Jil','PEN','mes','KIL','968574123','DXEWDWE@gmail.com');

-- Procedimiento para insertar en la tabla Docente
DELIMITER //
CREATE PROCEDURE Insertar_Docente(
    IN d_DNI INT,
    IN d_Primer_nombre VARCHAR(50),
	IN d_Segundo_nombre VARCHAR(50),
    IN d_Apellido_paterno VARCHAR(50),
    IN d_Apellido_materno VARCHAR(50),
    IN d_Numero_de_celular VARCHAR(9),
    IN d_Correo_electronico VARCHAR(100)
)
BEGIN
    INSERT INTO Docente (DNI, Primer_nombre, Apellido_paterno, Apellido_materno) 
    VALUES (d_DNI, d_Primer_nombre, d_Apellido_paterno, d_Apellido_materno);
    INSERT INTO Segundo_nombre_del_docente (Segundo_nombre, DNI) 
    VALUES (d_Segundo_nombre, d_DNI);
    INSERT INTO Numero_de_celular_del_docente (Numero_de_celular, DNI) 
    VALUES (d_Numero_de_celular, d_DNI);
    INSERT INTO Correo_electronico_del_docente (Correo_electronico, DNI) 
    VALUES (d_Correo_electronico, d_DNI);
END //
DELIMITER ;

CALL Insertar_Docente (75054548,'Jil','PEN','mes','KIL','968574123','DXEWDWE@gmail.com');

-- Procedimiento para insertar en la tabla Personal
DELIMITER //
CREATE PROCEDURE Insertar_Personal(
    IN p_DNI INT,
    IN p_Primer_nombre VARCHAR(50),
	IN p_Segundo_nombre VARCHAR(50),
    IN p_Apellido_paterno VARCHAR(50),
    IN p_Apellido_materno VARCHAR(50),
    IN p_Numero_de_celular VARCHAR(9),
    IN p_Correo_electronico VARCHAR(100)
)
BEGIN
    INSERT INTO Personal (DNI, Primer_nombre, Apellido_paterno, Apellido_materno) 
    VALUES (p_DNI, p_Primer_nombre, p_Apellido_paterno, p_Apellido_materno);
    INSERT INTO Segundo_nombre_del_personal (Segundo_nombre, DNI) 
    VALUES (p_Segundo_nombre, p_DNI);
    INSERT INTO Numero_de_celular_del_personal (Numero_de_celular, DNI) 
    VALUES (p_Numero_de_celular, p_DNI);
    INSERT INTO Correo_electronico_del_personal (Correo_electronico, DNI) 
    VALUES (p_Correo_electronico, p_DNI);
END //
DELIMITER ;

CALL Insertar_Personal (75054548,'Jil','PEN','mes','KIL','968574123','DXEWDWE@gmail.com');

-- Procedimiento para ingresar datos en la matricula
DELIMITER //
CREATE PROCEDURE InsertarMatricula(
    IN p_Id_de_matricula INT,
    IN p_Fecha DATE,
    IN p_Carrera VARCHAR(100),
    IN p_Monto DECIMAL(10, 2),
    IN p_Metodo_de_pago VARCHAR(50),
    IN p_DNI INT,
    IN p_Id_del_periodo INT
)
BEGIN
    INSERT INTO Matricula (Id_de_matricula, Fecha, Carrera, Monto, Metodo_de_pago, DNI, Id_del_periodo) 
    VALUES (p_Id_de_matricula, p_Fecha, p_Carrera, p_Monto, p_Metodo_de_pago, p_DNI, p_Id_del_periodo);
END //
DELIMITER ;

-- Procedimieento para insertar datos en los grupos
DELIMITER //
CREATE PROCEDURE InsertarGrupo(
    IN p_Id_del_grupo INT,
    IN p_Turno VARCHAR(15),
    IN p_Numero_de_estudiantes INT,
    IN p_Nombre VARCHAR(30),
    IN p_Codigo_del_aula INT
)
BEGIN
    INSERT INTO Grupo (Id_del_grupo, Turno, Numero_de_estudiantes, Nombre, Codigo_del_aula) 
    VALUES (p_Id_del_grupo, p_Turno, p_Numero_de_estudiantes, p_Nombre, p_Codigo_del_aula);
END //
DELIMITER ;

-- Procedimiento para la asistencia del docente
DELIMITER //
CREATE PROCEDURE InsertarAsistenciaDocente(
    IN p_DNI INT,
    IN p_Id_de_la_asignatura INT
)
BEGIN
    INSERT INTO Docente_encargado_de_la_Asignatura (DNI, Id_de_la_asignatura) 
    VALUES (p_DNI, p_Id_de_la_asignatura);
END //
DELIMITER ;

-- Procedimiento para la asistencia del estudiante
DELIMITER //
CREATE PROCEDURE InsertarAsistenciaClases(
    IN p_Id_de_las_clases INT,
    IN p_Fecha DATE,
    IN p_Id_de_la_asignatura INT,
    IN p_Id_del_grupo INT
)
BEGIN
    INSERT INTO Clases (Id_de_las_clases, Fecha, Id_de_la_asignatura, Id_del_grupo) 
    VALUES (p_Id_de_las_clases, p_Fecha, p_Id_de_la_asignatura, p_Id_del_grupo);
END //
DELIMITER ;

-- Procedimiento para insertar datos en examen cepre
DELIMITER //
CREATE PROCEDURE InsertarExamenCEPRE(
    IN p_Id_de_examen INT,
    IN p_Fecha DATE,
    IN p_Hora_de_inicio TIME,
    IN p_Hora_de_termino TIME,
    IN p_Numero_de_preguntas INT,
    IN p_Id_del_periodo INT
)
BEGIN
    INSERT INTO Examen_CEPRE (Id_de_examen, Fecha, Hora_de_inicio, Hora_de_termino, Numero_de_preguntas, Id_del_periodo) 
    VALUES (p_Id_de_examen, p_Fecha, p_Hora_de_inicio, p_Hora_de_termino, p_Numero_de_preguntas, p_Id_del_periodo);
END //
DELIMITER ;

-- Procedimiento para insertar al estudiante en un grupo
DELIMITER //
CREATE PROCEDURE InsertarGrupoEstudiante(
    IN p_DNI INT,
    IN p_Id_del_grupo INT
)
BEGIN
    INSERT INTO Grupo_al_que_pertenece_el_estudiante (DNI, Id_del_grupo) 
    VALUES (p_DNI, p_Id_del_grupo);
END //
DELIMITER ;

-- Procedimiento para cosultar datos del estudiante
DELIMITER //
CREATE PROCEDURE Buscar_nombre_del_estudiante (in Letras varchar (40))
BEGIN
	SELECT * FROM Estudiante
    WHERE Primer_nombre OR Apellido_paterno OR Apellido_materno LIKE concat('%', Letras, '%');
END //
DELIMITER ;

CALL Buscar_nombre_del_estudiante ('PAZ');

-- Procedimiento para cosultar datos del docente
DELIMITER //
CREATE PROCEDURE Buscar_nombre_y_apellido_del_docente (in Letras3 varchar (40))
BEGIN
	SELECT * FROM Docente
    WHERE Primer_nombre OR Apellido_paterno OR Apellido_materno LIKE concat('%', Letras3, '%');
END //
DELIMITER ;

CALL Buscar_nombre_y_apellido_del_docente ('Clavijo');

-- Procedimiento para cosultar datos del personal
DELIMITER //
CREATE PROCEDURE Buscar_nombre_y_apellido_del_personal (in Letras3 varchar (40))
BEGIN
	SELECT * FROM Personal
    WHERE Primer_nombre OR Apellido_paterno OR Apellido_materno LIKE concat('%', Letras3, '%');
END //
DELIMITER ;

CALL Buscar_nombre_y_apellido_del_personal ('hif');

-- Procedimiento para cosultar el codigo de matricula
DELIMITER //
CREATE PROCEDURE Buscar_id_matricula (in Letras3 int)
BEGIN
	SELECT * FROM Matricula
    WHERE Id_de_matricula LIKE concat('%', Letras3, '%');
END //
DELIMITER ;
